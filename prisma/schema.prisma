generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Kpis {
  id                  String   @id
  name                String
  evaluation_criteria String
  condition           String
  target_result       Float
  divide_number       Float
  sum_result          String?
  excellence          String
  area_level          String
  ssj_department      String
  ssj_pm              String?
  moph_department     String?
  kpi_type            String
  grade               String?
  template_url        String?
  last_synced_at      DateTime @default(now())
  created_at          DateTime @default(now())
  updated_at          DateTime @updatedAt

  @@index([ssj_department, area_level])
  @@index([kpi_type])
  @@map("kpis")
}

model KpiReport {
  money_year Int
  area_name  String
  kpi_id     String
  kpi_name   String
  kpi_target Float?
  result_oct Float?
  result_nov Float?
  result_dec Float?
  result_jan Float?
  result_feb Float?
  result_mar Float?
  result_apr Float?
  result_may Float?
  result_jun Float?
  result_jul Float?
  result_aug Float?
  result_sep Float?
  sum_result String?
  rate       Float?
  status     String?
  updated_at DateTime @updatedAt

  @@id([money_year, area_name, kpi_id])
  @@map("kpi_report")
}
